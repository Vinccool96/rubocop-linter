{
  "name": "rubocop-linter",
  "version": "2.0.0",
  "private": true,
  "description": "Lint ruby files via rubocop",
  "main": "lib/index.js",
  "scripts": {
    "build": "ncc build src/index.ts --out lib",
    "watch": "ncc build src/index.ts --watch --out lib",
    "lint": "eslint --ext .ts src",
    "lint:fix": "eslint --ext .ts --fix src",
    "prepare": "husky install",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vinccool96/rubocop-linter.git"
  },
  "keywords": [
    "github",
    "action",
    "version",
    "npm",
    "node"
  ],
  "author": "Federico Grandi <fgrandi30@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EndBug/add-and-commit/issues"
  },
  "homepage": "https://github.com/EndBug/add-and-commit#readme",
  "dependencies": {
    "@actions/core": "^1.9.1",
    "@actions/exec": "^1.1.1",
    "actions-toolkit": "github:EndBug/actions-toolkit#core-actions",
    "octokit": "^2.0.7",
    "simple-git": "^3.7.1",
    "string-argv": "^0.3.1",
    "yaml": "^2.1.1"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vercel/ncc": "^0.34.0",
    "all-contributors-cli": "^6.20.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.3.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "yamljs": "^0.3.0"
  }
}
